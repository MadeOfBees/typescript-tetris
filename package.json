{
    "name": "typescript-tetris",
    "version": "0",
    "description": "A Typescript/NextJS/Tailwind port of the classic game Tetris with a scoreboard system using MongoDB, Express and Node JS.",
    "main": "server/server.js",
    "engines": {
        "node": "16.17.1"
    },
    "scripts": {
        "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
        "build": "cd client && npm run build",
        "start": "cd server && npm run start"
      },
    "keywords": [
        "game",
        "mongodb",
        "react",
        "node",
        "typescript",
        "tailwind",
        "nextjs",
        "puzzle",
        "express",
        "tetris"
    ],
    "author": "MadeOfBees",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^5.3.0"
    }
}
